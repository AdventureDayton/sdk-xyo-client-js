{
  "name": "@xyo-network/sdk-xyo-client-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "version": "2.1.38",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@xyo-network/sdk-xyo-js": "^2.1.36",
    "@xyo-network/sdk-xyo-mongo-js": "^2.1.17",
    "axios": "^0.21.1",
    "elliptic": "^6.5.4",
    "is-ip": "^3.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.0",
    "sha.js": "^2.4.11",
    "tslib": "^2.3.1"
  },
  "scripts": {
    "build": "rm -r -f dist && yarn deps && yarn lint && yarn lint-pkg && yarn compile",
    "compile": "tsc -p tsconfig.build.json && rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn build && yarn publish",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint-pkg": "npmPkgJsonLint .",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "test": "yarn build && yarn jest ./src",
    "test:bw": "yarn build && yarn jest ./src/BoundWitness",
    "test:payload": "yarn build && yarn jest ./src/Payload",
    "test:api": "yarn build && yarn jest ./src/ArchivistApi",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./dist",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune",
    "up": "yarn ncu",
    "updo": "yarn ncu -u && yarn install",
    "deps": "yarn depcheck"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/elliptic": "^6.4.13",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16",
    "@types/sha.js": "^2.4.0",
    "@xylabs/eslint-config": "^2.0.16",
    "ajv": "^8.6.2",
    "depcheck": "^1.4.2",
    "eslint": "^7.32.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.0.6",
    "npm-check-updates": "^11.8.3",
    "npm-package-json-lint": "^5.2.2",
    "npm-package-json-lint-config-default": "^3.0.0",
    "rollup": "^2.56.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "ts-prune": "^0.9.2",
    "typescript": "^4.3.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}